<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="CommodityShoppingcart" namespace="/api/mallShoppingcartAction" extends="default">
		<action name="index" class="mallShoppingcartAction" method="index"></action>
		<action name="list" class="mallShoppingcartAction" method="list"></action>	
		<action name="add" class="mallShoppingcartAction" method="add"></action>	
		<action name="update" class="mallShoppingcartAction" method="update"></action>
		<action name="delete" class="mallShoppingcartAction" method="delete"></action>
		<action name="listView" class="mallShoppingcartAction" method="listView">
			<result name="success">/WEB-INF/page/mall/orders/commodityorders_inf_list.jsp</result>
			<result name="input">/WEB-INF/page/mall/orders/commodityorders_inf_details.jsp</result>
		</action>	
		
		<action name="getUserSpCount" class="mallShoppingcartAction" method="getUserSpCount"></action>	
		
	</package>
</struts>